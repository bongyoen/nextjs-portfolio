{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "presentation": "off",
    "max-classes-per-file": ["error", 2]
  }
}
